{
  "name": "jsonni-cli",
  "version": "1.0.0",
  "description": "Manipulate data with ES6, Lodash or fromfrom, from your command line",
  "main": "./bin/index.js",
  "scripts": {
    "build": "tsc && chmod a+x bin/index.js",
    "dev": "tsc -w",
    "lint": "eslint src/**/*.ts",
    "test": "npm run build && jest",
    "prepublish": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hannupekka/jsonni-cli.git"
  },
  "keywords": [
    "JSON",
    "transform",
    "lodash",
    "fromfrom"
  ],
  "author": "Hannu-Pekka Heinäjärvi <hannupekka@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hannupekka/jsonni-cli/issues"
  },
  "engines": {
    "node": "^10"
  },
  "homepage": "https://github.com/hannupekka/jsonni-cli#readme",
  "dependencies": {
    "commander": "^2.19.0",
    "consola": "^2.5.8",
    "convert-to-object": "0.0.4",
    "fromfrom": "^1.1.1",
    "get-stdin": "^6.0.0",
    "insomnia-prettify": "^0.1.8",
    "jsonminify": "^0.4.1",
    "lodash": "^4.17.11",
    "safe-eval": "^0.4.1",
    "stringify-object": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/get-stdin": "^5.0.1",
    "@types/jest": "^24.0.11",
    "@types/jsonminify": "^0.4.0",
    "@types/lodash": "^4.14.123",
    "@types/stringify-object": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "babel-jest": "^24.5.0",
    "eslint": "^5.16.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "typescript": "^3.4.1"
  },
  "bin": {
    "jsonni": "./bin/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  }
}
